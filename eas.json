{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://nvitclock.com/api"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://nvitclock.com/api"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
